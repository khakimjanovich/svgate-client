{
  "name": "khakimjanovich/svgate",
  "type": "library",
  "description": "Tiny, framework-agnostic PHP SDK for SVGate JSON-RPC API",
  "license": "MIT",
  "require": {
    "php": ">=8.4",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/http-message": "^2.0",
    "psr/log": "^3.0",
      "ext-ctype": "*"
  },
  "require-dev": {
    "nyholm/psr7": "^1.6",
    "phpunit/phpunit": "^11.0",
    "laravel/pint": "^1.16"
  },
  "scripts": {
    "test": "phpunit",
    "format": "pint",
    "format:check": "pint --test"
  },
  "autoload": {
    "psr-4": {
      "Khakimjanovich\\SVGate\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Khakimjanovich\\SVGate\\Tests\\": "tests/"
    }
  }
}
